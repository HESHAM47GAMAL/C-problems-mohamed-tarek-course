#include <stdio.h>

int main()
{
	int num1 , num2 ,num3;
	printf("Enter num1 : ");
	fflush(stdout);
	scanf("%d",&num1);
	printf("Enter num2 : ");
	fflush(stdout);
	scanf("%d",&num2);
	printf("Enter num3 : ");
	fflush(stdout);
	scanf("%d",&num3);
	int small = num1;
	if(small > num2)
		small  = num2;
	if(small > num3)
		small = num3 ;

	printf("Small num  : ",small );




	return 0;
}
