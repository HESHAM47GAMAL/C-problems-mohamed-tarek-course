#include <stdio.h>

int main ()
{
	int num = 10235 ;
	int digit = 0 ;
//	printf("Enter your number to count digits : ");
//	fflush(stdout );
//	scanf("%d",&num);
	while(num != 0)
	{
		digit ++;
		num %= 10;
	}
	printf("Number of digits  = %d",digit );

}
