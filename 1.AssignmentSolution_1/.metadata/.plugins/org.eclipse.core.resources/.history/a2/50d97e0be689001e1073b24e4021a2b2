#include <stdio.h>
#include <math.h>
int main()
{
	int number ;
	printf("Enter number want to check is Prime : ");
	fflush(stdout);
	scanf("%d",&number);
	int div =  sqrt(number);
	for(short checker = 2 ; checker <= div ; checker++)
	{
		if(number % checker == 0)
		{
			printf("It's prime number ");
			break;
		}
	}
	return 0;
}
