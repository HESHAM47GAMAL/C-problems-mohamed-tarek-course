#include <stdio.h>

int main ()
{
	int numFac ;
	printf("Enter number :  ");
	fflsush(stdout);
	scanf("%d",&numFac);
	int result = 1 ;
	for(short i = 2 ; i <= numfac ; i++)
	{
		result *= i;
	}
	printf("result : %d",result);
	fflush(stdout);

	return  0;
}
