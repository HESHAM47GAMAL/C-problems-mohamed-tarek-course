#include <stdio.h>

char power3(int);

int main()
{
	int num;
	printf("Enter number to check is power of 3 :"),fflush(stdout);
	scanf("%d",&num);



	return 0;
}

char power3(int N)
{
	while(1)
	{
		if(N % 3 == 0)
		{
			if(N == 3)
				return 1;
			N /= 3;
		}
		else
			return 0;
	}
}

