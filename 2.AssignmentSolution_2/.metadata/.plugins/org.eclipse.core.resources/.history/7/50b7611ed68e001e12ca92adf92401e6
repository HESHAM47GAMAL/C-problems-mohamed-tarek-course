#include<stdio.h>

int Fibonacci(int);

int main()
{
	int num;
	printf("Enter Fibonacci number :  "),fflush(stdout);
	scanf("%d",&num);


	return 0;
}

int Fibonacci(int n)
{
	if(n == 0 || n == 1)
		return n;
	return Fibonacci(n-1) + Fibonacci(n-2);
}
