#include <stdio.h>

int cube(int num)
{
	return num * num * num;
}

int main()
{

	int num;
	printf("Enter number to get cube :"),fflush(stdout);
	scanf("%d",&num);
	int result = cub(num);
	printf("result : %d",result);


	return 0;
}

