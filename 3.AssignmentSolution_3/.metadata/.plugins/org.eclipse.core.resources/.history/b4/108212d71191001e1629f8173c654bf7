#include <stdio.h>

void convertUpperOnly(char *);

int main ()
{
	char String [30];
	printf("Enter string to convert only upper latter to lower : "),fflush(stdout);
	fget(String);
	convertUpperOnly(String);
	printf("converted string : %s",String);


	return 0
}

void convertUpperOnly(char *S)
{
	if(*S >= 'A' && *S <= 'Z')
	{
		*S += 32;
	}
	S++;

}

