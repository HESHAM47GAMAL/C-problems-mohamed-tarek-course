#include <stdio.h>

void SeriesP2(int);

int main ()
{
	printf("Series Sequence : "),fflush(stdout);
	SeriesP2(5);



	return 0;
}

void SeriesP2(int Nth)
{
	long  series_index = 1;
	for(short i = 0 ; i <= Nth ; i++)
	{
		printf("%li ",series_index),fflush(stdout);
		series_index += 2;
	}

}

